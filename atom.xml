---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>m7a.me</title>
  <link href="http://m7a.me/"/>
  <link type="application/atom+xml" rel="self" href="http://m7a.me/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://m7a.me/</id>
  <author>
    <name>Yuki Morishita</name>
    <email>mor.yuki@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://m7a.me{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://m7a.me{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <content type="html">{{ post.excerpt | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
